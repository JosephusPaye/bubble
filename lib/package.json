{
  "name": "bubble",
  "author": "Josephus Paye II",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/bubble.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "generate-parser": "pegjs -o src/peg/generated-parser.js src/peg/bubble.pegjs",
    "build": "tsdx build --format cjs,esm,umd",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "transform": {
      ".pegjs": "jest-raw-loader",
      ".(ts|tsx)": "ts-jest",
      ".(js|jsx)": "babel-jest"
    }
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/pegjs": "^0.10.1",
    "husky": "^4.2.1",
    "jest-raw-loader": "^1.0.1",
    "pegjs": "^0.10.0",
    "rollup-plugin-string": "^3.0.0",
    "tsdx": "^0.12.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  }
}
